<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="WorkIcon.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:mi="http://www.aathifmahir.com/dotnet/2022/maui/icons">

    <ScrollView>
        <VerticalStackLayout Padding="30,0" Spacing="25">

            <mi:MauiIcon Icon="{mi:FontAwesomeBrand Windows}" />
            <mi:MauiIcon Icon="{mi:Material ABC}" />

            <Label mi:MauiIcon.Value="{mi:Fluent Icon=WindowSettings20, IconColor=Blue, FontOverride=True}" />

            <Image mi:MauiIcon.Value="{mi:FontAwesomeBrand Icon=Windows}" Aspect="Center" />

            <Button mi:MauiIcon.Value="{mi:FontAwesomeBrand Icon=Windows}" />

        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
